<resources>
    <declare-styleable name="CodeViewer">
        <attr name="wrapLines" format="boolean" />
        <attr name="keywordColor" format="color" />
        <attr name="variableColor" format="color" />
        <attr name="primitiveColor" format="color" />
        <attr name="literalColor" format="color" />
        <attr name="tabLength" format="integer" />
        <attr name="language" format="enum">
            <enum name="java" value="0" />
            <enum name="cSharp" value="1"/>
            <enum name="ruby" value="2" />
        </attr>
    </declare-styleable>
</resources>
